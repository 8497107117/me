{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-boolean-value": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/self-closing-comp": 0,
    "react/no-danger": 0,
    "arrow-parens": 0,
    "arrow-body-style": 0,
    "brace-style": [
      2,
      "stroustrup",
      {
        "allowSingleLine": true
      }
    ],
    "comma-dangle": 0,
    "no-case-declarations": 0,
    "no-param-reassign": 0,
    "no-tabs": 0,
    "no-fallthrough": 0,
    "prefer-const": 0,
    "quote-props": [
      "error",
      "as-needed",
      {
        "unnecessary": false
      }
    ],
    "import/no-dynamic-require": 0,
    "global-require": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-quotes": 0
  },
  "env": {
    "browser": true,
    "node": true
  }
}